<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.propify.challenge.AddressMapper">

    <insert id="insert">
INSERT INTO address ...
    </insert>

    <select id="search" resultType="com.propify.challenge.Address">
SELECT * FROM address;
    </select>

    <select id="findById" resultType="com.propify.challenge.Address">
SELECT * FROM address WHERE id = #{id};
    </select>

</mapper>
